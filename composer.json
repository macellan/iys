{
    "name": "macellan/iys",
    "description": "Iys library for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Enver Cigal",
            "email": "enver.cigal@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2|^7.0.1",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0|^3.2",
        "laravel/pint": "^1.21",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^7.1|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.1|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Macellan\\Iys\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Macellan\\Iys\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "@test:style",
            "@test:unit"
        ],
        "test:unit": "vendor/bin/phpunit",
        "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text",
        "test:coverage:html": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-html coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test:style": [
            "@format",
            "@analyse"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
